 <% unless @bankpasswords.size >= 5 && current_user.premium_user == false %>
  <%= link_to new_bank_password_path do %>
    <i class="fa-solid fa-circle-plus" id="agregar-bank"></i>
  <% end %>
<% end %>
<div class="cards-all" data-searchcards-target="listcard">
  <% bank_passwords.each_with_index do |bankpassword, num| %>
    <%= render 'card_info', bankpassword: bankpassword, num: num %>
  <% end %>
</div>
